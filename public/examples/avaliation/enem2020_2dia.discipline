                    "options": [
                        {
                            "hash": "",
                            "option_text": "H<sub>2</sub>"
                        },
                        {
                            "hash": "",
                            "option_text": "Cl<sub>2</sub>"
                        },
                        {
                            "hash": "",
                            "option_text": "CO"
                        },
                        {
                            "hash": "",
                            "option_text": "CO<sub>2</sub>"
                        },
                        {
                            "hash": "",
                            "option_text": "HCl"
                        }
                    ],
                    "correct_answer": "E"
